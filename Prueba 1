# Script de ejemplo para R Studio

# Cargar bibliotecas
library(ggplot2)

# Crear datos de muestra
set.seed(123)
datos <- data.frame(
  x = rnorm(100),
  y = rnorm(100)
)

# Crear gr치fico de dispersi칩n
ggplot(datos, aes(x = x, y = y)) +
  geom_point() +
  ggtitle("Gr치fico de Dispersi칩n de Ejemplo") +
  xlab("Eje X") +
  ylab("Eje Y")
